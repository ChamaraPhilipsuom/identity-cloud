<%
var log = new Log();

include("../controllers/wsUtil.jag");
var util = require("../util/utility.jag");

var serverUrl = util.getServerUrl();
serverUrl = serverUrl + "/CloudDirectoryAdminService";
var ws = require('ws');
var webService = new ws.WSRequest();
var i18n = session.get("i18n");
var agenturl = request.getParameter("url");

testConnection();
function testConnection(){
    try{
        webService = new ws.WSRequest();
        var result = callWSReturnJSON(webService, serverUrl, "urn:testConnection", agenturl, null);
        print(result);
    }catch(e){
        print(e);
        log.error(e);
    }

}

%>
